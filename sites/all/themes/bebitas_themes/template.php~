<?php 
function bebitas_theme() {
    $items = array();
    // create custom user-login.tpl.php
    $items['user_login'] = array(
        'render element' => 'form', 'path' => drupal_get_path('theme', 'bebitas_theme') . '/templates',
        'template' => 'page__login'
    );
    return $items;
}
?>
